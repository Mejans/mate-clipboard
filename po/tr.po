#: src/clipman-preferences.c:81
msgid "Clipboard Manager Preferences"
msgstr "Pano Yöneticisi Tercihleri"

#: src/clipman-preferences.c:85
msgid "_Close"
msgstr "_Kapat"

#. General section
#: src/clipman-preferences.c:94
msgid "General"
msgstr "Genel"

#: src/clipman-preferences.c:101
msgid "History size:"
msgstr "Geçmiş boyutu:"

#: src/clipman-preferences.c:107
msgid "Maximum number of items to keep in history"
msgstr "Geçmişte tutulacak en fazla öğe sayısı"

#: src/clipman-preferences.c:111
msgid "Keep clipboard content when source closes"
msgstr "Kaynak kapandığında pano içeriğini koru"

#: src/clipman-preferences.c:113
msgid "Restore clipboard content when the application that copied it closes"
msgstr "Kopyalayan uygulama kapandığında pano içeriğini geri yükle"

#: src/clipman-preferences.c:117
msgid "Confirm before clearing history"
msgstr "Geçmişi temizlemeden önce onayla"

#: src/clipman-preferences.c:121
msgid "Automatically paste when selecting from history"
msgstr "Geçmişten seçerken otomatik yapıştır"

#. Clipboard section
#: src/clipman-preferences.c:125
msgid "Clipboard Sources"
msgstr "Pano Kaynakları"

#: src/clipman-preferences.c:128
msgid "Track primary selection (middle-click paste)"
msgstr "Birincil seçimi izle (orta tıkla-yapıştır)"

#: src/clipman-preferences.c:130
msgid "Also save text selected with the mouse"
msgstr "Fareyle seçilen metni de kaydet"

#: src/clipman-preferences.c:134
msgid "Synchronize clipboard and primary selection"
msgstr "Panoyu ve birincil seçimi eşitle"

#: src/clipman-preferences.c:136
msgid "Keep both selections synchronized"
msgstr "Her iki seçimi eşitlenmiş tut"

#. Content types section
#: src/clipman-preferences.c:140
msgid "Content Types"
msgstr "İçerik Türleri"

#: src/clipman-preferences.c:143
msgid "Save images to history"
msgstr "Görüntüleri geçmişe kaydet"

#: src/clipman-preferences.c:147
msgid "Save file paths to history"
msgstr "Dosya yollarını geçmişe kaydet"

#: src/clipman-preferences.c:151
msgid "Show image previews in history"
msgstr "Geçmişte görüntü önizlemelerini göster"

#. Filter section
#: src/clipman-preferences.c:155
msgid "Filtering"
msgstr "Filtreleme"

#: src/clipman-preferences.c:158
msgid "Exclude pattern:"
msgstr "Dışlama deseni:"

#: src/clipman-preferences.c:163
msgid "Regular expression"
msgstr "Düzenli ifade"

#: src/clipman-preferences.c:165
msgid "Text matching this pattern will not be saved (e.g., passwords)"
msgstr "Bu desenle eşleşen metin kaydedilmez (örn. parolalar)"

#: data/mate-clipman.desktop.in:3
msgid "Clipboard history manager for MATE"
msgstr "MATE için pano geçmişi yöneticisi"

#: data/mate-clipman.desktop.in:9
msgid "clipboard;paste;copy;history;"
msgstr "pano;yapıştır;kopyala;geçmiş;"

#: data/mate-clipman-autostart.desktop.in:3
msgid "Start clipboard manager on login"
msgstr "Oturum açılışında pano yöneticisini başlat"

#: data/org.mate.panel.ClipmanApplet.mate-panel-applet.desktop.in:5
msgid "Clipman Applet Factory"
msgstr "Clipman Uygulamacık Fabrikası"

#: data/org.mate.panel.ClipmanApplet.mate-panel-applet.desktop.in:6
msgid "Factory for the clipboard manager applet"
msgstr "Pano yöneticisi uygulamacığı için fabrika"

#: data/org.mate.panel.ClipmanApplet.mate-panel-applet.desktop.in:9
msgid "Clipboard Manager"
msgstr "Pano Yöneticisi"

#: data/org.mate.panel.ClipmanApplet.mate-panel-applet.desktop.in:10
msgid "View and manage clipboard history"
msgstr "Pano geçmişini görüntüle ve yönet"
